{
	"name": "express-ts",
	"version": "1.0.0",
	"main": "app.ts",
	"license": "MIT",
	"scripts": {
		"start": " node dist/app.js",
		"dev": "nodemon src/app.ts",
		"build": "tsc -p .",
		"lint": "eslint . --ext .ts"
	},
	"dependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.38",
		"@types/sequelize": "^4.28.13",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"eslint": "^8.16.0",
		"express": "^4.18.1",
		"express-async-errors": "^3.1.1",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.20.1",
		"ts-node": "^10.8.0",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.2"
	},
	"_moduleAliases": {
		"@/resources": "dist/resources",
		"@/utils": "dist/utils",
		"@/middleware": "dist/middleware",
		"@/controllers": "dist/controllers",
		"@/models": "dist/models",
		"@/routes": "dist/routes",
		"@/db": "dist/db"
	},
	"eslintConfig": {
		"rules": {
			"no-unused-vars": [
				"error",
				{
					"argsIgnorePattern": "next"
				}
			]
		}
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/jsonwebtoken": "^8.5.8"
	}
}
